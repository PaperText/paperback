{
	"name": "paperback devcontainer",
  // compose settings
  "dockerComposeFile": [
		"../compose.yaml",
		"./compose.extend.yaml"
	],
  "service": "paperback",
	"workspaceFolder": "/root/paperback/",
  "forwardPorts": [7878, 5432, 7474],
  // IDE settings
  "settings": {
		"python.defaultInterpreterPath": "/usr/local/bin/python",
    "scm.defaultViewMode": "tree",
	},
  // IDE extensions
  "extensions": [
		"ms-python.python",
		"EditorConfig.EditorConfig",
	],
}
